	note = {Publisher: Frontiers},
	keywords = {PLSR, Biochemical composition, feed, Food, FT-NIR, fuel, Sorghum grains},
	file = {Full Text:/home/ryan/Zotero/storage/HEWA37P4/Ejaz et al. - 2021 - Sorghum Grains Grading for Food, Feed, and Fuel Us.pdf:application/pdf},
