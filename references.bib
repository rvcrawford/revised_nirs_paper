	keywords = {Differentiation, Digital differentiator, Property, Savitzky–Golay},
	pages = {122--136},
	file = {ScienceDirect Snapshot:/home/ryan/Zotero/storage/834JFDLD/S1051200404000727.html:text/html},
}

