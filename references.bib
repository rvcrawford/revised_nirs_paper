	author = {Stordrange, Laila and Libnau, Fred Olav and Malthe-Sørenssen, Dick and Kvalheim, Olav M.},
	year = {2002},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/cem.754},
	keywords = {near-infrared spectroscopy, multivariate calibration, preprocessing, variable selection},
	pages = {529--541},
	file = {Full Text PDF:/home/ryan/Zotero/storage/2VLE5NII/Stordrange et al. - 2002 - Feasibility study of NIR for surveillance of a pha.pdf:application/pdf;Snapshot:/home/ryan/Zotero/storage/59G5H7IQ/cem.html:text/html},
